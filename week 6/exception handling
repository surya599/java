public class excep
{
    public static void main(String [] args)
    {
        int i = 9;
        int j = 3;
        int k = 0;
        try
        {
            k = i/j;
        }
        catch(Exception e)
        {
            System.out.println("something went wrong");
        }

        System.out.println(k);
        System.out.println("end of program");
    }
}
